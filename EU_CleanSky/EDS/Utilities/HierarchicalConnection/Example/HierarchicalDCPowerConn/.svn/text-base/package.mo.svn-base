within EU_CleanSky.EDS.Utilities.HierarchicalConnection.Example;
package HierarchicalDCPowerConn "Pass a DC electrical power connector along the model hierarchy."
extends Base;


redeclare connector extends HierarchicalConnector "Graphical Creation"

  Modelica.Electrical.Analog.Interfaces.Pin pin[2] "The pins"
    annotation (Placement(transformation(extent={{4,-16},{36,16}})));
  annotation (
    defaultComponentPrefixes="protected outer",
    defaultComponentName="hierarchy",
    Icon(coordinateSystem(preserveAspectRatio=false, extent={{-100,-100},{100,
            100}}), graphics={Rectangle(
          extent={{-100,100},{0,-100}},
          lineColor={0,0,255},
          fillColor={0,0,255},
          fillPattern=FillPattern.Solid), Rectangle(
          extent={{0,100},{100,-100}},
          lineColor={0,0,255},
          fillColor={255,255,255},
          fillPattern=FillPattern.Solid)}),
    Diagram(coordinateSystem(preserveAspectRatio=false, extent={{-100,-100},{
            100,100}}), graphics),
    Documentation(info="<html>
<p>Note that the &QUOT;redeclare connector extends&QUOT; syntax begins a block of code which is always stored in the same file as derived class which contains it.</p>
</html>"));
end HierarchicalConnector;

end HierarchicalDCPowerConn;
