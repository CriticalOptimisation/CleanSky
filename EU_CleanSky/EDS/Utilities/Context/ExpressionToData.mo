within EU_CleanSky.EDS.Utilities.Context;
block ExpressionToData
  "Block defines a Data record using the record construction function."

  output DataOutput d "Record constructor function call" annotation (Dialog(
        group="Time varying output record"), Placement(transformation(extent={{
            100,-10},{120,10}}), iconTransformation(extent={{100,-10},{120,10}})));
  annotation (preferredView="text",Icon(coordinateSystem(preserveAspectRatio=
            false, extent={{-100,-100},{100,100}}), graphics={Rectangle(extent=
          {{-100,30},{100,-30}}, lineColor={0,0,0}),Text(
          extent={{-98,20},{100,-20}},
          lineColor={0,0,0},
          textString="%d"),Text(
          extent={{-96,94},{94,50}},
          lineColor={0,0,255},
          fillColor={255,213,170},
          fillPattern=FillPattern.Solid,
          textString="%name")}));
end ExpressionToData;
