          lineColor={255,0,128}),Ellipse(extent={{-48,22},{-12,-16}}, lineColor=
           {255,0,128}),Ellipse(extent={{-6,22},{30,-16}}, lineColor={255,0,128}),
          Line(
          points={{-16,14},{-2,14}},
          color={255,0,128},
          smooth=Smooth.None),Line(
          points={{-46,10},{6,46},{14,46},{16,38}},
          color={255,0,128},
          smooth=Smooth.None),Line(
          points={{28,8},{52.832,25.1914}},
          color={255,0,128},
          smooth=Smooth.None)}), Diagram(coordinateSystem(extent={{-60,-60},{60,
            60}})));
end Reader;
