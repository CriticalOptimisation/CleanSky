within EU_CleanSky.EDS.Utilities.Context;
connector Connector
  "This connector can be used inside other connector to propagate a context."
  extends Base;
  Data context;

  annotation (Documentation(info="<html>
<p>Users shall derive from ContextConnector and replace the Context record by another record derived from ContextBase. The new Context record must contain all the contextual shared information.</p>
<p>From the Modelica point of view, ContextConnector and all the derivative connectors are &QUOT;overdetermined connectors&QUOT;.</p>
</html>"), Icon(coordinateSystem(preserveAspectRatio=false, extent={{-100,-100},
            {100,100}}), graphics={Polygon(
          points={{0,100},{-100,0},{0,-100},{100,0},{0,100}},
          smooth=Smooth.None,
          fillColor={255,0,255},
          fillPattern=FillPattern.Solid,
          pattern=LinePattern.None),Polygon(
          points={{42,42},{22,56},{-24,56},{-36,42},{-36,-40},{-12,-54},{16,-54},
            {42,-40},{42,-20},{22,-20},{22,-28},{16,-34},{-8,-34},{-18,-30},{-18,
            36},{-12,42},{18,42},{24,36},{24,26},{42,26},{42,42}},
          smooth=Smooth.None,
          fillColor={255,255,255},
          fillPattern=FillPattern.Solid,
          pattern=LinePattern.None,
          lineColor={0,0,0})}));
end Connector;
